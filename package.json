{
  "name": "markdown-blockquote-toggler",
  "displayName": "Markdown Blockquote Toggler",
  "description": "Toggle markdown blockquotes on/off",
  "version": "0.0.1",
  "publisher": "EliYing",
  "repository": {
    "type": "git",
    "url": "https://github.com/ellisy0/markdown-blockquote-toggler.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "mdBlockquoteToggler.toggle",
        "key": "ctrl+'",
        "mac": "cmd+'",
        "when": "editorTextFocus && resourceLangId == 'markdown'"
      }
    ],
    "commands": [
      {
        "command": "mdBlockquoteToggler.toggle",
        "title": "Toggle Blockquote"
      }
    ],
    "activationEvents": [
      "onCommand:mdBlockquoteToggler.toggle"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
